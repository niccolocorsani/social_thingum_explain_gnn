        rdfs:label       "Reactor" , "Tank" ;
        rdfs:subClassOf  <https://saref.etsi.org/saref4bldg/Reactor> , <https://saref.etsi.org/saref4bldg/PhysicalObject> .

<http://www.disit.org/saref4bldg-ext/Flow>
        a                owl:Class ;
        rdfs:comment     "A generic Flow from and to a Physical Object, usually achived with pipes." ;
        rdfs:label       "Flow" ;
        rdfs:subClassOf  [ a                            owl:Restriction ;
                           owl:minQualifiedCardinality  "1"^^xsd:nonNegativeInteger ;
                           owl:onClass                  <https://saref.etsi.org/saref4bldg/PhysicalObject> ;
                           owl:onProperty               <http://www.disit.org/saref4bldg-ext/startIn>
                         ] ;
        rdfs:subClassOf  [ a                            owl:Restriction ;
                           owl:minQualifiedCardinality  "1"^^xsd:nonNegativeInteger ;
                           owl:onClass                  <https://saref.etsi.org/saref4bldg/PhysicalObject> ;
                           owl:onProperty               <http://www.disit.org/saref4bldg-ext/endIn>
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  <https://saref.etsi.org/saref4bldg/PhysicalObject> ;
                           owl:onProperty     <http://www.disit.org/saref4bldg-ext/startIn>
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  <https://saref.etsi.org/saref4bldg/PhysicalObject> ;
                           owl:onProperty     <http://www.disit.org/saref4bldg-ext/endIn>
                         ] .

<http://www.disit.org/altair/resource/PhysicalObject>
